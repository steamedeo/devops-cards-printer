{
  "manifestVersion": 1,
  "id": "steamedeo",
  "version": "1.0.20",
  "name": "Devops Card Printer",
  "scopes": ["vso.work", "vso.extension.data_write"],
  "description": "Print work items from the context menu.",
  "publisher": "steamedeo",
  "public": false,
  "icons": {
    "default": "static/img/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": ["Azure Repos"],
  "tags": ["print", "work items", "cards", "physical cards"],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/steamedeo/devops-cards-printer/issues"
    },
    "repository": {
      "uri": "https://github.com/steamedeo/devops-cards-printer"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/steamedeo/devops-cards-printer"
  },
  "branding": {
    "color": "rgb(27,160,225)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "static/img",
      "addressable": true
    },
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "static/pcards.html",
      "addressable": true
    },
    {
      "path": "node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "node_modules/office-ui-fabric-js/dist/js",
      "addressable": true,
      "packagePath": "lib/js"
    },
    {
      "path": "node_modules/office-ui-fabric-js/dist/css",
      "addressable": true,
      "packagePath": "lib/css"
    }
  ],
  "contributions": [
    {
      "id": "print-work-item",
      "type": "ms.vss-web.action",
      "description": "Print a work item.",
      "targets": ["ms.vss-work-web.work-item-context-menu"],
      "properties": {
        "uri": "static/pcards.html"
      }
    }
  ]
}
